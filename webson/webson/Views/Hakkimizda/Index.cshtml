@model IEnumerable<webson.Models.Hakkimizda>

@{
    ViewBag.Title = "Hakkımızda";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table">
    <tr>
        <th>
            Hakkımızda Açıklama
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.Raw(item.Aciklama)
        </td>
        <td>
            @Html.ActionLink("Düzenle", "Edit", new { id=item.HakkimizdaId })
        </td>
    </tr>
}

</table>
